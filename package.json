{
  "name": "training-manager",
  "version": "1.0.0",
  "description": "Training Manager",
  "scripts": {
    "lint": "eslint ./libs ./client ./server --ext .js --ext .jsx",
    "lintfix": "npm run lint -- --fix",
    "testonly": "mocha client/**/tests/*.js --compilers js:babel-register",
    "test": "npm run lint && npm run testonly",
    "test-watch": "npm run testonly -- --watch --watch-extensions js,jsx",
    "deploy-staging": "meteor deploy aep-training",
    "deploy-production": "cd .deploy/production && mup deploy",
    "stage": "npm run deploy-staging",
    "prod": "npm run deploy-production",
    "start": "meteor"
  },
  "author": "Alpha Shuro",
  "dependencies": {
    "babel-core": "6.x.x",
    "babel-plugin-react-require": "2.x.x",
    "babel-polyfill": "6.x.x",
    "babel-preset-es2015": "6.x.x",
    "babel-preset-react": "6.x.x",
    "babel-root-slash-import": "1.x.x",
    "chai": "3.x.x",
    "enzyme": "1.x.x",
    "eslint-plugin-react": "^3.15.0",
    "mocha": "2.x.x",
    "react-addons-test-utils": "^0.14.6",
    "sinon": "1.17.x",
    "babel-eslint": "^6.0.0",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-meteor": "^6.6.7",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "classnames": "^2.2.3",
    "domready": "^1.0.8",
    "eslint": "^2.3.0",
    "eslint-plugin-react": "^3.16.1",
    "lodash": "^4.6.1",
    "mantra-core": "^1.3.0",
    "meteor-node-stubs": "^0.2.1",
    "moment": "^2.11.2",
    "react": "^0.14.6",
    "react-big-calendar": "^0.9.8",
    "react-bootstrap": "^0.28.2",
    "react-bootstrap-datetimepicker": "^0.0.22",
    "react-datetime": "^2.0.3",
    "react-dom": "^0.14.6",
    "react-mounter": "^1.0.0",
    "source-map-url": "^0.4.0",
    "underscore": "^1.8.3"
  },
  "private": true
}
